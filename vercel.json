{
  "version": 2,
  "builds": [
    {
      "src": "api/flask.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/flask.py" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
